int i ; 1 ;

while i < 100
    if i <= 1
        print "[i] is a prime number\n" ;
        add i ; 1 ;
        continue ;
    end

    int flag ; 1 ;
    int j ; 2 ;

    while j < i / 2 + 1
        if i % j == 0
            set flag ; 0 ;
            break ;
        end
        add j ; 1 ;
    end

    if flag
        print "[i] is a prime number\n" ;
    end

    add i ; 1 ;
end
