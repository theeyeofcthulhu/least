int i ; 1 ;

while i < 100
    if i <= 1
        print "[i] is a prime number\n" ;
        add i ; 1 ;
        continue ;
    end

    int is_prime ; 1 ;
    int j ; 2 ;

    while j < i / 2 + 1
        if i % j == 0
            set is_prime ; 0 ;
            break ;
        end
        add j ; 1 ;
    end

    if is_prime
        print "[i] is a prime number\n" ;
    end

    add i ; 1 ;
end
